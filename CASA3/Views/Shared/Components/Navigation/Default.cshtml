@model List<Core.DTOs.ProjectDto>

@if (Model != null && Model.Any())
{
    @foreach (var project in Model)
    {
        <a asp-controller="Home" asp-action="ProjectDetails" asp-route-id="@project.Id" class="block px-5 py-3.5 text-gray-300 hover:bg-[#0F172A] hover:text-[#60A5FA] transition-colors border-b border-[#334155] last:border-b-0 first:rounded-t-lg last:rounded-b-lg text-sm">
            @project.Name
        </a>
    }
}

